#include <bits/stdc++.h>
using namespace std;

class Node
{
 public:
 int data;
 Node * next;
 
 Node(int value){
     data = value;
     next = NULL;
     
 }
};


int main()
{
 Node* Head, *Tail;
 Head =Tail= NULL;
  
  int arr[]= {2,4,6,8,10};
  //Insert the value at end
for(int i=0; i<5; i++){
    // linkedlist empty 
    
    if(Head==NULL)
    {
        Head=new Node(arr[i]);
        Tail = Head;
    }
    //or exist
    else 
    {
        Tail->next=new Node(arr[i]);
        Tail = Tail->next;
    }
 
}

//1st delete
Node* temp = Head;
Head=Head->next;
delete temp;
//

//output
 Node* Temp = Head;
 while(Temp)
 {
     cout<<Temp->data<<" ";
     Temp=Temp->next;
 }
 
 
    return 0;
}
