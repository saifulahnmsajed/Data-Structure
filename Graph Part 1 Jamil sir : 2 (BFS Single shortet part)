#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n, e; cin>>n>>e;
    vector<vector<int> > adj(n+1);

    for(int i=0; i<e; i++)
    {
        int u,v; cin>>u>>v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }

    int s; cin>> s;
    vector<int> level(n+1, -1);
    vector<int> parent(n+1, -1);
    level[s] = 0;
    queue<int> q;

    q.push(s);

    while(!q.empty())
    {
        int u = q.front();
        q.pop(); // u = 6
        for(int v : adj[u])
        {
            //for 6 = 5,7,9
            if(level[v] == -1)
            {
                parent[v] = u;
                level[v] = level[u] + 1;
                q.push(v);
            }
        }
    }
    //Print
    for(int i=1; i<n+1; i++)
    {
        cout<<level[i] << " ";
    }
    cout<< endl;
    int u,v; cin>>v;
    vector<int> path;
    path.push_back(v);
    while(parent[v] != -1)
    {
        path.push_back(parent[v]);
        v = parent[v];
    }
    for(auto i:path)
    {
        cout<< i << " -> ";
    }
    return 0;
}
